/* pages/setting/index.wxss */
page{
  background-color: #eee;
}
.content{
  width: 100%;
}
.nav-common{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  margin:20rpx auto;
}
.nav-common image{
  padding-left:40rpx;
  margin-top: 25rpx;
  float: left;
}
.nav-common label{
  display: block;
  height: 80rpx;
  width: 70%;
  float: left;
  font-size: 30rpx;
   text-indent: 20rpx; 
}
.backlogin{
  display: block;
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  margin: 0 auto;
  background-color: #d81e06;
  color: #fff;
  font-weight: bold;
  position: absolute;
  bottom:40rpx;
}