/* pages/logined/index.wxss */
page{
  background-color: #f6f4f4;
}
.userinfo{
  width: 100%;
  height: 200rpx;
  display: flex; 
  flex-direction: column; 
  align-items: center;
  background-color: #fff;
}

.user_header{
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  margin: 20rpx;
}
.navigator-list{
  width: 100%;
  height: auto;
}
.nav-common{
  width: 90%;
  margin: 20rpx auto;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
  border-radius: 10rpx;
  font-size: 30rpx;
  padding-left: 20rpx;
  border: 2rpx solid #eee;
}